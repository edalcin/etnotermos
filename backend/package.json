{
  "name": "etnotermos-backend",
  "version": "1.0.0",
  "description": "EtnoTermos backend - Ethnobotanical terms management system integrated with etnoDB",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/start.js",
    "dev:public": "node --watch src/contexts/public/server.js",
    "dev:admin": "node --watch src/contexts/admin/server.js",
    "start:public": "node src/contexts/public/server.js",
    "start:admin": "node src/contexts/admin/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "ethnobotany",
    "controlled-vocabulary",
    "mongodb",
    "z39.19",
    "etnodb"
  ],
  "author": "EtnoTermos Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "mongodb": "^6.3.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "multer": "^1.4.5-lts.1",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^9.1.4",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "js-yaml": "^4.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
